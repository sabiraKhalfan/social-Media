\n  const handleChange=(e)=>{\r\n    setData({...data,[e.target.name]:e.target.value})\r\n  }\r\n\r\n\r\nconst handleSubmit=(e)=>{\r\ne.preventDefault();\r\n\r\n\r\nif(IsSignUp){\r\ndata.password === data.confirmpass?\r\n dispatch(signUp(data)):\r\n  setConfirmPass(false);\r\n  \r\n}\r\nelse {\r\n  dispatch(logIn(data))\r\n}\r\n}\r\n\r\nconst resetForm=()=>{\r\n  setConfirmPass(true);\r\n  setData(\r\n  {firstname: \"\",\r\n  lastname: \"\",\r\n  username: \"\",\r\n  password: \"\",\r\n  confirmpass: \"\",\r\n})\r\n}\r\n\r\n\r\n\r\n\r\n  return (\r\n    <div className=\"Auth\" >\r\n      <div className=\"a-right\">\r\n\r\n        <form className='infoForm authForm' onSubmit={handleSubmit}>\r\n\r\n\r\n          <h3>{IsSignUp ? \"SignUp\" : \"Login\"}</h3>\r\n\r\n          {IsSignUp &&\r\n\r\n            <div className='inputname'>\r\n              <input \r\n              type=\"text\" \r\n              placeholder='First Name' \r\n              className='infoInput' name='firstname' \r\n               value={data.firstname} \r\n               onChange={handleChange} />\r\n\r\n              <input \r\n              type=\"text\"\r\n               placeholder='Last Name' \r\n               className='infoInput' \r\n               name='lastname' \r\n               onChange={handleChange} />\r\n\r\n            </div>\r\n\r\n          }\r\n          <div className='infoInput1' >\r\n            <div>\r\n              <input \r\n              type=\"text\" \r\n              className=\"infoInput\"\r\n               name='username' \r\n               placeholder='UserName' \r\n               style={{ width: \"90%\" }} \r\n               onChange={handleChange} />\r\n            </div>\r\n            <div className='infoInput2'>\r\n              <input \r\n              type=\"password\" \r\n              className='infoInput'\r\n               name='password' placeholder='Password' \r\n               onChange={handleChange}\r\n               value={data.password} />\r\n\r\n\r\n\r\n              {IsSignUp &&\r\n                \r\n                  <input type=\"password\"\r\n                    className='infoInput'\r\n                     name='confirmpass' \r\n                     placeholder='Confirm Password'\r\n                      onChange={handleChange}\r\n                      value={data.confirmpass}/>\r\n\r\n                \r\n              }\r\n            </div>\r\n          </div>\r\n          <span\r\n            style={{\r\n              color: \"red\",\r\n              fontSize: \"16px\",\r\n              alignSelf: \"center\",\r\n              marginRight: \"5px\",\r\n              display: confirmPass ? \"none\" : \"block\",\r\n            }}\r\n          >\r\n            *Confirm password is not same\r\n          </span>\r\n          <div>\r\n            <span\r\n             style={{ \r\n              fontSize: \"12px\",\r\n              cursor:\"pointer\",\r\n            }} \r\n            onClick ={()=>{\r\n              resetForm();\r\n              setSignUp((prev)=>!prev)\r\n            }}\r\n            >\r\n             {IsSignUp  ?  \"Already Have an Account! Login\":\"Don't Have an account! SignUp\"} \r\n            </span>\r\n          </div>\r\n          \r\n          <button className='button infoButton' type='submit' >{IsSignUp?\"Sign Up\":\"Login\"}</button>\r\n\r\n        </form>\r\n      </div>\r\n    </div>\r\n  )\r\n}\r\n\r\nexport default Auth","D:\\socialmedia\\client\\src\\Pages\\Profile\\Profile.jsx",[],[],"D:\\socialmedia\\client\\src\\PostSide\\PostSide.jsx",[],[],"D:\\socialmedia\\client\\src\\RightSide\\RightSide.jsx",[],[],"D:\\socialmedia\\client\\src\\ProfileLeft\\ProfileLeft.jsx",[],[],"D:\\socialmedia\\client\\src\\Components\\ProfileCard\\ProfileCard.jsx",[],[],"D:\\socialmedia\\client\\src\\Components\\ProfileSide\\ProfileSide.jsx",[],[],"D:\\socialmedia\\client\\src\\TrendCard\\TrendCard.jsx",[],[],"D:\\socialmedia\\client\\src\\Posts\\Posts.jsx",[],[],"D:\\socialmedia\\client\\